<div class="container-fluid">
  <div class="row">
    <div class="col-sm-12 title">
      <% if (!category) { %>
        <h1>All Projects:</h1>
      <% } else { %>
        <h1>All Projects in <%= category.escape('title') %>:</h1>
      <% } %>

      <br>
      <select>
        <% categories.forEach(function (lcategory) { %>
          <option
            value="<%= lcategory.id %>"
            <%= lcategory.id == category.id ? "selected" : "" %> >
            <%= lcategory.escape('title') %>
          </option>
        <% }) %>
      </select>
    </div>
  </div>
</div>
<div id="projects">
</div>
